{
  "hosting": [
    {
      "site": "lwai-3bac8",
      "public": "public",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "/favicon.ico",
          "destination": "/favicon.ico"
        },
        {
          "source": "/robots.txt",
          "destination": "/robots.txt"
        },
        {
          "source": "/:lang/:id",
          "function": "directToCase"
        },
        {
          "source": "/etraining/direct/:id",
          "function": "directToTrainings"
        },
        {
          "source": "/etraining/direct/:id/:pricesInclVat",
          "function": "directToTrainings"
        },
        {
          "source": "/etraining/direct/:id/:pricesInclVat/:specialcode",
          "function": "directToTrainings"
        }
      ],
      "redirects": [
        {
          "source": "/",
          "destination": "https://conversation.alicialabs.com",
          "type": 301
        }
      ]
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "emulators": {
    "projectId":"lwai-3bac8",
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080,
      "host": "localhost",
      "persist": true,
      "dataDir": "./firestore-data"
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true,
    "auth": {
      "port": 9099
    }
  }
}
